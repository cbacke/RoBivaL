@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

@prefix rfp: <http://w3id.org/RoBivaL/FDOProfile/> .

rfp:FundamentalProfile
    a sh:NodeShape ;
    dcterms:conformsTo rfp:FundamentalProfile ;
    sh:closed true ;
    sh:property [
        sh:path rdf:type ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:hasValue sh:NodeShape ;
    ], [
        sh:path dcterms:conformsTo ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ], [
        sh:path sh:closed ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( false true ) ;
    ], [
        sh:path sh:targetNode ;
        sh:minCount 0 ;
    ], [
        sh:path sh:property ;
        sh:minCount 2 ;
    ], [
        sh:path sh:property ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedValueShape [
            sh:path sh:path ;
            sh:hasValue rdf:type ;
        ] ;
    ], [
        sh:path sh:property ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        sh:qualifiedValueShape [
            sh:path sh:path ;
            sh:hasValue dcterms:conformsTo ;
        ] ;
    ] ;
.
